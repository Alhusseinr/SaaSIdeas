{
  "name": "reddit-ingest-railway",
  "version": "1.0.0",
  "description": "High-volume Reddit ingestion service for Railway",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:enrichment": "node dist/enrichment-service.js",
    "dev": "ts-node src/server.ts",
    "dev:enrichment": "ts-node src/enrichment-service.ts",
    "deploy": "railway up"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.3",
    "concurrently": "^9.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.0",
    "express": "^4.18.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
